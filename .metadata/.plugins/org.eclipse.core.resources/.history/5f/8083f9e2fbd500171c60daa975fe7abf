	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int vector[][]=new int [10][10];
		int suma[]=new int [10];
		String comodin="%3d ";
		for (int i = 0; i < suma.length; i++) {
			suma[i]=suma[i]+1;
			System.out.printf(comodin,suma[i]);
			System.out.println();
		}
		
		
		
	}

}
