/**
 * @author usuario
 *
 */
public class Ejercicio40 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int matriz[][][]= new int[4][5][3];
		int cont=1;
		for (int p = 1; p < matriz[0][0].length +1; p++) {
			System.out.println("Pág " + p);
			for (int f = 1; f < matriz[0].length +1; f++) {
				for (int c = 1; c < matriz.length +1; c++) {
					matriz[p][f][c]=cont;
					cont ++;
					System.out.print(matriz[p][f][c]);
				}
				System.out.println();
			}
			System.out.println("" + "");
		}
	}

}
