			mc2= (int) (Math.random()*5)+1;
			comodin2++;
			
			for (long i = 0; i < 100000000L ; i++) {
				
			}
		}
		if (c1 > c2) {
			System.out.println("Ha ganado el caballo 1");
		} else {
