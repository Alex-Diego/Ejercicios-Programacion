/**
 * @author usuario
 *
 */
public class main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String baraja="1♥2♥3♥4♥5♥6♥7♥8♥9♥J♥Q♥K♥A♥"
					+ "1♦2♦3♦4♦5♦6♦7♦8♦9♦J♦Q♦K♦A♦"
					+ "1♣2♣3♣4♣5♣6♣7♣8♣9♣J♣Q♣K♣A♣"
					+ "1♠2♠3♠4♠5♠6♠7♠8♠9♠J♠Q♠K♠A♠";
		String carta="";
		int num=0;
		for (int i = 0; i < 52 ; i++) {
			num = (int) (Math.random()*baraja.length());
			if (num%2!=0) {
				num--;
			}
			carta=baraja.substring(num, num+2);
			if (carta.equals("1♥")) {
				carta="10♥";
			}else if(carta.equals("1♦")){
				carta="10♦";
			}else if(carta.equals("1♣")){
				carta="10♣";
			}
			else if(carta.equals("1♠")){
				carta="10♠";
			}
			System.out.print(carta+" ");
			if (i%13==0) {
				System.out.println();
			}
			baraja=baraja.substring(0,num)+baraja.substring(num+2);
		}
		
	}

}
